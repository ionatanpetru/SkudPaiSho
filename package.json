{
	"requires": true,
	"lockfileVersion": 1,
	"dependencies": {
		"boardgame.io": "^0.50.2",
		"html2canvas": "^1.4.1",
		"jquery": "^3.7.1",
		"lz-string": "^1.5.0",
		"package.json": "^2.0.1",
		"three": "^0.182.0"
	},
	"name": "skud-pai-sho",
	"description": "The Garden Gate - the home of Pai Sho. Play online and learn more at [The Garden Gate - SkudPaiSho.com](https://skudpaisho.com).",
	"version": "1.0.0",
	"type": "module",
	"source": "index.html",
	"scripts": {
		"test": "vitest run",
		"test:watch": "vitest",
		"test:ui": "vitest --ui",
		"clean": "rm -rf dist .parcel-cache",
		"start": "parcel index.html --open",
		"build": "npm run clean && parcel build index.html --public-url ./",
		"build:deploy": "npm run build && ./deploy-prep.sh",
		"start-ugly": "./minify.sh && parcel --open",
		"build-ugly": "npm run clean && ./minify.sh && parcel build --public-url ./",
		"build-ugly:deploy": "npm run build-ugly && ./deploy-prep.sh",
		"lint": "eslint ./js/*.js"
	},
	"staticFiles": [
		{
			"staticPath": "images",
			"staticOutPath": "images",
			"env": {
				"NODE_ENV": "production"
			}
		},
		{
			"staticPath": "images",
			"staticOutPath": "images",
			"env": {
				"NODE_ENV": "development"
			}
		},
		{
			"staticPath": "style",
			"staticOutPath": "style",
			"env": {
				"NODE_ENV": "production"
			}
		},
		{
			"staticPath": "style",
			"staticOutPath": "style",
			"env": {
				"NODE_ENV": "development"
			}
		},
		{
			"staticPath": "sw.js",
			"staticOutPath": "sw.js"
		}
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/thejambi/SkudPaiSho.git"
	},
	"keywords": [],
	"author": "",
	"license": "GPL-3.0",
	"bugs": {
		"url": "https://github.com/thejambi/SkudPaiSho/issues"
	},
	"homepage": "https://github.com/thejambi/SkudPaiSho#readme",
	"devDependencies": {
		"@parcel/packager-raw-url": "^2.12.0",
		"@parcel/transformer-webmanifest": "^2.12.0",
		"buffer": "^6.0.3",
		"eslint": "^8.57.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-plugin-import": "^2.29.1",
		"happy-dom": "^20.3.7",
		"parcel": "^2.12.0",
		"parcel-reporter-static-files-copy": "^1.5.3",
		"process": "^0.11.10",
		"vitest": "^4.0.18"
	}
}
